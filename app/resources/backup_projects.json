  "profiles": [
    {
      "id": "full",
      "label": "Full backup",
      "use_network_time": true,
      "skip_zip": false,
      "max_file_kb": 256,
      "include_steps": [
        "trees",
        "code_txt",
        "ts_tsx_md_bundles",
        "full_ts_tsx_bundle",
        "configs",
        "tsconfig_bundle",
        "api_group_bundles",
        "paths",
        "keyword_search"
      ]
    },
    {
      "id": "quick",
      "label": "Quick backup (no ZIP, trees + TS bundles only)",
      "use_network_time": false,
      "skip_zip": true,
      "max_file_kb": 128,
      "include_steps": [
        "trees",
        "ts_tsx_md_bundles",
        "paths"
      ],
      "no_root_files": true
    },
    {
      "id": "typescript_only",
      "label": "TypeScript/TSX Only",
      "use_network_time": false,
      "skip_zip": true,
      "max_file_kb": 256,
      "include_steps": [
        "trees",
        "full_ts_tsx_bundle"
      ],
      "allowed_ts_extensions": [
        ".ts",
        ".tsx"
      ],
      "no_root_files": true
    },
    {
      "id": "python_only",
      "label": "Python Only",
      "use_network_time": false,
      "skip_zip": true,
      "max_file_kb": 256,
      "include_steps": [
        "trees",
        "code_txt"
      ],
      "allowed_full_extensions": [
        ".py",
        ".pyi",
        ".json",
        ".yaml",
        ".yml",
        ".toml"
      ],
      "no_root_files": true
    },
    {
      "id": "frontend_focus",
      "label": "Frontend Focus (React/Vue/Angular)",
      "use_network_time": false,
      "skip_zip": false,
      "max_file_kb": 256,
      "include_steps": [
        "trees",
        "code_txt",
        "ts_tsx_md_bundles"
      ],
      "allowed_full_extensions": [
        ".ts",
        ".tsx",
        ".js",
        ".jsx",
        ".vue",
        ".svelte",
        ".css",
        ".scss",
        ".html"
      ]
    },
    {
      "id": "backend_focus",
      "label": "Backend Focus (NestJS/Express)",
      "use_network_time": false,
      "skip_zip": false,
      "max_file_kb": 256,
      "include_steps": [
        "trees",
        "code_txt",
        "api_group_bundles",
        "configs"
      ],
      "allowed_full_extensions": [
        ".ts",
        ".js",
        ".json",
        ".yaml",
        ".yml",
        ".env"
      ]
    },
    {
      "id": "configs_only",
      "label": "Config Files Only",
