  "search_sets": [
    {
      "id": "auth",
      "label": "Auth-related keywords",
      "keywords": [
        "auth",
        "login",
        "token",
        "jwt",
        "session",
        "password"
      ],
      "extensions": [
        ".ts",
        ".tsx",
        ".js",
        ".jsx"
      ]
    },
    {
      "id": "payments",
      "label": "Payments / checkout",
      "keywords": [
        "payment",
        "stripe",
        "checkout",
        "invoice",
        "billing"
      ],
      "extensions": [
        ".ts",
        ".tsx",
        ".js",
        ".jsx"
      ]
    },
    {
      "id": "api_endpoints",
      "label": "API Endpoints",
      "keywords": [
        "@Get",
        "@Post",
        "@Put",
        "@Delete",
        "@Patch",
        "router.",
        "app.get",
        "app.post"
      ],
      "extensions": [
        ".ts",
        ".js"
      ]
    },
    {
      "id": "database",
      "label": "Database Operations",
      "keywords": [
        "prisma",
        "mongoose",
        "sequelize",
        "typeorm",
        "knex",
        "findMany",
        "findOne",
        "create",
        "update",
        "delete"
      ],
      "extensions": [
        ".ts",
        ".js"
      ]
    },
    {
      "id": "hooks",
